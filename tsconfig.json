{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"types": ["@cloudflare/workers-types"],
		"baseUrl": ".",
		"paths": {
			"~/*": ["src/*"]
		},
		"allowJs": true
	}
}
